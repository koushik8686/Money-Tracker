{
    "version": 2,
    "build": {
        "env": {
            "PYTHON_VERSION": "3.11"
        }
    },
    "builds": [
        {
            "src": "./app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]   
}